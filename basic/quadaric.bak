(define (what-kind a b c)
  (if (and (rational? a) (rational? b) (rational c))
      (if (a = 0) 'degenerate
          (if (< (-(* b b)(* (* 4 a) c)) 0)) 'none
               (if (= (-(* b b)(* (* 4 a) c)) 0) ) 'one
                   (if (< (-(* b b)(* (* 4 a) c)) 0)) 'two -1)-1))