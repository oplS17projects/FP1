            <h3>
              <span>
                <@>
                  <class>
                    mw-headline
                  </class>
                  <id>
                    Pre-colonial
                  </id>
                </@>
                Pre-colonial
              </span>
              <span>
                <@>
                  <class>
                    mw-editsection
                  </class>
                </@>
                <span>
                  <@>
                    <class>
                      mw-editsection-bracket
                    </class>
                  </@>
